  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js --output-public-path='/build/static/'",
    "test": "node scripts/test.js",
    "mock": "json-server -w db.json -p 4200 -d 70 --id Id",
    "server": "concurrently \"npm run start\" \"npm run mock\"",
    "proxy": "node ./api-server.ts",
    "spServer": "concurrently --kill-others \"npm run proxy\" \"npm run start\""
  },
  "proxy": "http://localhost:8081",
